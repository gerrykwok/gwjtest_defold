[project]
title = gwjtest
version = 0.1
bundle_resources = /res/bundle_resource/
appname = defoldtest
environment = development

[bootstrap]
main_collection = /main/main.collectionc
render = /main/render/only.renderc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1136
height = 640

[physics]
scale = 0.02

[script]
shared_state = 1

[android]
