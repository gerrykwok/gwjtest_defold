[project]
title = gwjtest
version = 0.1

[bootstrap]
main_collection = /main/main.collectionc
render = /main/render/only.renderc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1136
height = 640

[physics]
scale = 0.02

[script]
shared_state = 1

[android]
app_icon_96x96 = /main/android/android_icon96.png
package = org.evakwok.defold.gwjtest
iap_provider = 
manifest = /cocosext/src/plmext/android/AndroidManifest.xml

[ios]
bundle_identifier = org.evakwok.defold.gwjtest
app_icon_114x114 = /main/android/android_icon96.png
infoplist = /cocosext/src/plmext/ios/Info.plist

[collection_proxy]
max_count = 128

[library]
include_dirs = gwjui

